package nl.photoreality.map_editor.model.vo 
{
	/**
	 * ...
	 * @author Miguel Fuentes // Triplebeam 2010
	 */
	public class SettingsVO
	{
		private var _version		:String;
		private var _lineThickness	:uint;
		private var _lineColor		:uint;
		public function SettingsVO(version:String){
			_version = version;
		}
		
		public function get version():String { return _version; }
		
		public function set version(value:String):void {
			_version = value;
		}
		
		public function get lineThickness():uint { return _lineThickness; }
		
		public function set lineThickness(value:uint):void {
			_lineThickness = value;
		}
		
		public function get lineColor():uint { return _lineColor; }
		
		public function set lineColor(value:uint):void {
			_lineColor = value;
		}
		
	}

}